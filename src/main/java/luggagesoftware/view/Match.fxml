            <Font name="UniSansHeavyItalicCAPS" size="15.0" />
         </font>
      </Button>
      <Button fx:id="foundLuggageBtn" layoutX="200.0" layoutY="90.0" mnemonicParsing="false" prefHeight="20.0" prefWidth="200.0" styleClass="buttonSelect" stylesheets="@../../css/style.css" text="Found luggage" textFill="WHITE">
         <font>
            <Font name="UniSansHeavyItalicCAPS" size="15.0" />
         </font>
      </Button>
      <Button fx:id="lostLuggageBtn" layoutY="90.0" mnemonicParsing="false" prefHeight="20.0" prefWidth="200.0" stylesheets="@../../css/style.css" text="Lost luggage" textFill="WHITE">
         <font>
            <Font name="UniSansHeavyItalicCAPS" size="15.0" />
         </font>
      </Button>
      <ImageView fitHeight="83.0" fitWidth="267.0" layoutX="7.0" layoutY="6.0" pickOnBounds="true" preserveRatio="true">
