            <Font name="UniSansHeavyItalicCAPS" size="15.0" />
         </font>
         <items>
            <MenuItem fx:id="profileBtn" mnemonicParsing="false" styleClass="btn" text="PROFIEL_TEXT" />
            <MenuItem fx:id="logOutBtn" mnemonicParsing="false" styleClass="btn" text="LOGOUT_TEXT" />
            <MenuItem fx:id="closeBtn" mnemonicParsing="false" styleClass="btn" text="CLOSE_PROGRAM_BTN" />
         </items>
      </MenuButton>
      <Button fx:id="travelerBtn" layoutX="600.0" layoutY="90.0" mnemonicParsing="false" prefHeight="20.0" prefWidth="200.0" stylesheets="@../../css/style.css" text="Travelers" textFill="WHITE">
