            <Font name="UniSansHeavyItalicCAPS" size="15.0" />
         </font>
      </Button>
      <Button id="editBtn" fx:id="deleteBtn" layoutX="220.0" layoutY="393.0" prefHeight="50.0" stylesheets="@../../css/style.css" text="DELETE_TEXT" textFill="WHITE">
         <font>
            <Font name="UniSansHeavyItalicCAPS" size="15.0" />
         </font>
      </Button>
      <Button fx:id="contributorsBtn" layoutX="600.0" layoutY="90.0" mnemonicParsing="false" prefHeight="20.0" prefWidth="200.0" stylesheets="@../../css/style.css" text="Contributors" textFill="WHITE">
         <font>
            <Font name="UniSansHeavyItalicCAPS" size="15.0" />
         </font>
      </Button>
      <Button fx:id="statisticsBtn" layoutX="400.0" layoutY="90.0" mnemonicParsing="false" prefHeight="20.0" prefWidth="200.0" stylesheets="@../../css/style.css" text="Statistics" textFill="WHITE">
