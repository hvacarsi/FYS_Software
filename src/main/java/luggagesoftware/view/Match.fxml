<AnchorPane id="AnchorPane" fx:id="verlorenBagagePane" prefHeight="450.0" prefWidth="800.0" style="-fx-background-color: white;" stylesheets="@../../css/style.css" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="luggagesoftware.MatchController">
   <children>
      <Button id="searchBtn" fx:id="exportPdf" layoutX="18.0" layoutY="393.0" prefHeight="50.0" stylesheets="@../../css/style.css" text="EXPORT_PDF_TEXT" textFill="WHITE">
